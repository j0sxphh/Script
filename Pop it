local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pop it trading", "Synapse")


-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Buy OffsaleItems")


MainSection:NewButton("Buy Dart", "Makes you buy a dart!", function()
    local args = {
    [1] = "Dart"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)

MainSection:NewButton("Buys Rainbow Star", "ButtonInfo", function()
    local args = {
    [1] = "Rainbow Star"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)

MainSection:NewButton("Buy Retro Bike", "Buy A retro bike", function()
    local args = {
    [1] = "Retro Bike"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

end)

MainSection:NewButton("Buy Demigaga", "Buys the demigaga", function()
    local args = {
    [1] = "Demogorgan"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

end)

MainSection:NewButton("Buy GreenOoze", "Buys A green ooze", function()
    local args = {
    [1] = "Green Ooze"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)

