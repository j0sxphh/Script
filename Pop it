-- Auto Buy
local AutoBuy = Window:NewTab("Auto Buy")
local AutoBuySection = AutoBuy:NewSection("AutoBuy")

AutoBuySection:NewButton("Auto Buy Dart (Buggy)", "Cant turn off soz:)", function()
    while wait()do
local args = {
    [1] = "Dart"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
end)


AutoBuySection:NewButton("Auto Buy Demigaga (Buggy)", "Cant turn off soz.", function()
    while wait()do
local args = {
    [1] = "Demogorgan"
}

game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end
end)

